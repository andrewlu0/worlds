{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "components/shaders/index.js", "components/shaders/LandscapeShader.js", "components/shaders/RefractionShader.js", "components/shaders/LandscapeShader.js", "components/post/Effects.js", "components/World2.js"  ],
  "exclude": [
    "node_modules"
  ]
}
